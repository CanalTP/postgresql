[core]
        repositoryformatversion = 0
        filemode = true
        bare = false
        logallrefupdates = true
        editor = vim
[remote "origin"]
        url = git@github.com:ANXS/postgresql.git
        fetch = +refs/heads/*:refs/remotes/origin/*
[remote "upstream"]
       url = git@github.com:CanalTP/postgresql.git
       fetch = +refs/heads/*:refs/remotes/upstream/*
[branch "master"]
        remote = upstream
        merge = refs/heads/master

